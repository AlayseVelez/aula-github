package Principal;

import java.util.ArrayList;
import java.util.List;

public class lista {

	public static void main(String[] args) {
		
		List<String> list = new ArrayList<>();
		
		list.add("Amanda");
		list.add("Lucas");
		list.add("Paulo");
        list.add("Luis");

        System.out.println(list.size());
        
        for(String s: list) {
        	System.out.println(s);
        }
	}

}
