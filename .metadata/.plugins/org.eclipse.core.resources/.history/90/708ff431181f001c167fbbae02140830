package entites;

public class Account {
	
	public int numero;
	public String nome;
	public double saldo;
	
	
	public Account(int numero, String nome) {
		
		this.numero = numero;
		this.nome = nome;
	}


	public Account(int numero, String nome, double saldo) {
		
		this.numero = numero;
		this.nome = nome;
		this.saldo = saldo;
	}
	
	public void deposito(double valor) {
		this.saldo+=valor;
	}
	public void saque(double valor) {
		this.saldo -= valor +5;
		
	}

}
